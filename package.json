{
  "name": "emit-interface",
  "version": "0.1.0",
  "private": false,
  "description": "Standalone EMIT Methane Plume Viewer app",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "source": "src/lib/index.ts",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "targets": {
    "emit-app": {
      "source": "public/index.html",
      "context": "browser"
    }
  },
  "homepage": ".",
  "scripts": {
    "build": "dotenv -- node ./run-parcel.js build build",
    "serve": "dotenv -- node ./run-parcel.js serve",
    "start": "parcel",
    "build-lib": "parcel build ./src/lib/index.ts --dist-dir dist",
    "clean": "rm -rf node_modules dist build yarn.lock .parcel-cache && yarn cache clean",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix-conflicts": "prettier --write . && eslint . --fix"
  },
  "dependencies": {
    "@date-io/moment": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "dotenv-cli": "^8.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.0",
    "@swc/helpers": "^0.5.3",
    "@turf/bbox-polygon": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/length": "^7.2.0",
    "chart.js": "^4.4.3",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-zoom": "^2.0.1",
    "d3": "^7.9.0",
    "luxon": "^3.5.0",
    "mapbox-gl": "^3.7.0",
    "mapboxgl-timeline": "^1.0.2",
    "moment": "^2.30.1",
    "parcel": "2.15.2",
    "parcel-resolver-tspaths": "^0.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-loader-spinner": "^6.1.6",
    "react-resizable-panels": "^2.1.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.3",
    "web-vitals": "^5.0.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.15.2",
    "@parcel/packager-ts": "2.15.2",
    "@parcel/transformer-typescript-types": "2.15.2",
    "@parcel/transformer-webmanifest": "2.15.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "typescript": ">=3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/US-GHG-Center/custom-emit-interface.git"
  }
}